$('#star').raty({
    readOnly: true,
    score: <%= @recipe.average_rating %>,
    path: '/assets/ratyrate'
  });

  $('#user_star').raty({
    score: <%= @rating.rate %>,
    path: '/assets/ratyrate',
    click: function(score, evt) {
      $.ajax({
        url: '/ratings/' + <%= @rating.id %>,
        type: 'PATCH',
        data: { score: score }
      }).done(function (data){});
    }
  });